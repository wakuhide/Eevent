<p id="notice"><%= notice %></p>

<h1>Listing Questionnaires</h1>

<table>
  <thead>
    <tr>
      <th>Cached satisfaction list</th>
      <th>Cached trigger list</th>
      <th>Body</th>
      <th>Template</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionnaires.each do |questionnaire| %>
      <tr>
        <td><%= questionnaire.cached_satisfaction_list %></td>
        <td><%= questionnaire.cached_trigger_list %></td>
        <td><%= questionnaire.body %></td>
        <td><%= questionnaire.template %></td>
        <td><%= link_to 'Show', questionnaire %></td>
        <td><%= link_to 'Edit', edit_questionnaire_path(questionnaire) %></td>
        <td><%= link_to 'Destroy', questionnaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionnaire', new_questionnaire_path %>
